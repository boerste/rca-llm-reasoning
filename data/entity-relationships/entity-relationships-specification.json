{
    "note": "Entities and relationships grounded in 24765-2017 - ISO/IEC/IEEE International Standard - Systems and software engineering--Vocabulary",
    "entities": [
        {
            "id": "Service",
            "definition": "An aggregation of software that satisfies an end use function."
        },
        {
            "id": "Service_Instance",
            "definition": "A specific and identifiable runtime execution of a Service.",
            "attributes": "type, log_alerts, metric_alerts"
        },
        {
            "id": "Host",
            "definition": "A virtual or physical computer, hardware device, or environment where software items or programs are deployed, installed, or executed.",
            "examples": "Virtual machine (VM), container, server, node"
        },
        {
            "id": "Coordination_Manager",
            "definition": "A component that manages metadata, state synchronization, and coordination tasks.",
            "examples": "ZooKeeper, Consul"
        },
        {
            "id": "Database",
            "definition": "An organized persistent collection of data and information that allows for its retrieval.",
            "examples": "MySQL, key-value store, file"
        },
        {
            "id": "Cache",
            "definition": "An organized non-persistent collection of data and information that allows for its retrieval.",
            "examples": "redis"
        },
        {
            "id": "Cache_Instance",
            "definition": "A specific and identifiable runtime execution of a Cache."
        }
    ],
